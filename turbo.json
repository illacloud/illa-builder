{
  "$schema": "https://turbo.build/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "build"
      ]
    },
    "build-cloud": {
      "dependsOn": [
        "build"
      ]
    },
    "build-self": {
      "dependsOn": [
        "build"
      ]
    }
  }
}