{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "ts-check": {
      "dependsOn": [
        "^ts-check"
      ]
    },
    "preview": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "build"
      ]
    },
    "build-cloud": {
      "dependsOn": [
        "build"
      ]
    },
    "build-self": {
      "dependsOn": [
        "build"
      ]
    },
    "build-self-cloud": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "dev"
      ]
    }
  }
}